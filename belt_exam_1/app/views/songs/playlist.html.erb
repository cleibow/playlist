<h1>Others who added <%= @song.artist %> - <%= @song.title %></h1>
<table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">No of times song has been added</th>
          </tr>
        </thead>
        <tbody>
            <% @song.users.distinct.each do |user| %>
          <tr>
            <td><a href="<%= user_path user.id %>"><%= user.first_name %> <%= user.last_name %></a></td>
            <td><%= user.likes.where(song_id: @song.id).length %></td>
          </tr>
          <% end %>
        </tbody>
      </table>